@article{Shi,
title = {Option pricing for Heston model with tempered fractional Brownian motion},
journal = {Results in Applied Mathematics},
volume = {27},
pages = {100623},
year = {2025},
issn = {2590-0374},
doi = {https://doi.org/10.1016/j.rinam.2025.100623},
url = {https://www.sciencedirect.com/science/article/pii/S2590037425000871},
author = {Zhengguang Shi},
keywords = {Heston model, Hawkes process, Fractional Brownian motion, Fourier-cosine method, Option pricing},
abstract = {In this paper, the Heston model with tempered fractional Brownian motion is derived by modified Hawkes processes. Based on the affine technique, we present the characteristic function under this model and prove the existence, uniqueness, and regularity of the solution to the tempered fractional Riccati equation. In addition, by combining with the Fourier cosine method, we derive a numerical formula for pricing European options under this model. Numerical results demonstrate the convergence of both the Adams method and the Fourier cosine method.}
}

